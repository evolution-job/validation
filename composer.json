{
    "name": "php-xapi/validation",
    "type": "library",
    "description": "Experience API domain object validation",
    "keywords": ["xAPI", "Experience API", "Tin Can API", "validator", "validation"],
    "homepage": "https://github.com/php-xapi/validation",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Flothmann",
            "homepage": "https://github.com/xabbuh"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "php-xapi/model": "^1.0 || ^2.0 || ^3.0",
        "symfony/config": "^2.7 || ^3.4 || ^4.0",
        "symfony/validator": "^2.7 || ^3.4 || ^4.0"
    },
    "require-dev": {
        "doctrine/annotations": "~1.0",
        "php-xapi/test-fixtures": "^1.0",
        "ramsey/uuid": "^2.9 || ^3.0",
        "symfony/phpunit-bridge": "^3.4 || ^4.0",
        "symfony/property-access": "^2.7 || ^3.4 || ^4.0"
    },
    "minimum-stability": "dev",
    "conflict": {
        "xabbuh/xapi-validator": "*"
    },
    "autoload": {
        "psr-4": {
            "Xabbuh\\XApi\\Validator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xabbuh\\XApi\\Validator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
